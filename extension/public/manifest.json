{
  "manifest_version": 3,
  "name": "Url phishing checking",
  "description": "Checking url is malicious or not",
  "version": "1.2.0",
  "icons": { "128": "logo.png" },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  }
}